[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "الْحَاقَّةُ"
  },
  {
    "id": 2,
    "name": "مَا الْحَاقَّةُ"
  },
  {
    "id": 3,
    "name": "وَمَا أَدْرَاكَ مَا الْحَاقَّةُ"
  },
  {
    "id": 4,
    "name": "كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ"
  },
  {
    "id": 5,
    "name": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ"
  },
  {
    "id": 6,
    "name": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ"
  },
  {
    "id": 7,
    "name": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا فَتَرَى الْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ"
  },
  {
    "id": 8,
    "name": "فَهَلْ تَرَىٰ لَهُمْ مِنْ بَاقِيَةٍ"
  },
  {
    "id": 9,
    "name": "وَجَاءَ فِرْعَوْنُ وَمَنْ قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ"
  },
  {
    "id": 10,
    "name": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَابِيَةً"
  },
  {
    "id": 11,
    "name": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ"
  },
  {
    "id": 12,
    "name": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ"
  },
  {
    "id": 13,
    "name": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ"
  },
  {
    "id": 14,
    "name": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً"
  },
  {
    "id": 15,
    "name": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ"
  },
  {
    "id": 16,
    "name": "وَانْشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ"
  },
  {
    "id": 17,
    "name": "وَالْمَلَكُ عَلَىٰ أَرْجَائِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ"
  },
  {
    "id": 18,
    "name": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنْكُمْ خَافِيَةٌ"
  },
  {
    "id": 19,
    "name": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ"
  },
  {
    "id": 20,
    "name": "إِنِّي ظَنَنْتُ أَنِّي مُلَاقٍ حِسَابِيَهْ"
  },
  {
    "id": 21,
    "name": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ"
  },
  {
    "id": 22,
    "name": "فِي جَنَّةٍ عَالِيَةٍ"
  },
  {
    "id": 23,
    "name": "قُطُوفُهَا دَانِيَةٌ"
  },
  {
    "id": 24,
    "name": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا أَسْلَفْتُمْ فِي الْأَيَّامِ الْخَالِيَةِ"
  },
  {
    "id": 25,
    "name": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِشِمَالِهِ فَيَقُولُ يَا لَيْتَنِي لَمْ أُوتَ كِتَابِيَهْ"
  },
  {
    "id": 26,
    "name": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ"
  },
  {
    "id": 27,
    "name": "يَا لَيْتَهَا كَانَتِ الْقَاضِيَةَ"
  },
  {
    "id": 28,
    "name": "مَا أَغْنَىٰ عَنِّي مَالِيَهْ ۜ"
  },
  {
    "id": 29,
    "name": "هَلَكَ عَنِّي سُلْطَانِيَهْ"
  },
  {
    "id": 30,
    "name": "خُذُوهُ فَغُلُّوهُ"
  },
  {
    "id": 31,
    "name": "ثُمَّ الْجَحِيمَ صَلُّوهُ"
  },
  {
    "id": 32,
    "name": "ثُمَّ فِي سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَاسْلُكُوهُ"
  },
  {
    "id": 33,
    "name": "إِنَّهُ كَانَ لَا يُؤْمِنُ بِاللَّهِ الْعَظِيمِ"
  },
  {
    "id": 34,
    "name": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ الْمِسْكِينِ"
  },
  {
    "id": 35,
    "name": "فَلَيْسَ لَهُ الْيَوْمَ هَاهُنَا حَمِيمٌ"
  },
  {
    "id": 36,
    "name": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ"
  },
  {
    "id": 37,
    "name": "لَا يَأْكُلُهُ إِلَّا الْخَاطِئُونَ"
  },
  {
    "id": 38,
    "name": "فَلَا أُقْسِمُ بِمَا تُبْصِرُونَ"
  },
  {
    "id": 39,
    "name": "وَمَا لَا تُبْصِرُونَ"
  },
  {
    "id": 40,
    "name": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
  },
  {
    "id": 41,
    "name": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَا تُؤْمِنُونَ"
  },
  {
    "id": 42,
    "name": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَا تَذَكَّرُونَ"
  },
  {
    "id": 43,
    "name": "تَنْزِيلٌ مِنْ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 44,
    "name": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ الْأَقَاوِيلِ"
  },
  {
    "id": 45,
    "name": "لَأَخَذْنَا مِنْهُ بِالْيَمِينِ"
  },
  {
    "id": 46,
    "name": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِينَ"
  },
  {
    "id": 47,
    "name": "فَمَا مِنْكُمْ مِنْ أَحَدٍ عَنْهُ حَاجِزِينَ"
  },
  {
    "id": 48,
    "name": "وَإِنَّهُ لَتَذْكِرَةٌ لِلْمُتَّقِينَ"
  },
  {
    "id": 49,
    "name": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنْكُمْ مُكَذِّبِينَ"
  },
  {
    "id": 50,
    "name": "وَإِنَّهُ لَحَسْرَةٌ عَلَى الْكَافِرِينَ"
  },
  {
    "id": 51,
    "name": "وَإِنَّهُ لَحَقُّ الْيَقِينِ"
  },
  {
    "id": 52,
    "name": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
  }
]