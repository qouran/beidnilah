    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ"
  },
  {
    "id": 2,
    "name": "خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ"
  },
  {
    "id": 3,
    "name": "اقْرَأْ وَرَبُّكَ الْأَكْرَمُ"
  },
  {
    "id": 4,
    "name": "الَّذِي عَلَّمَ بِالْقَلَمِ"
  },
  {
    "id": 5,
    "name": "عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ"
  },
  {
    "id": 6,
    "name": "كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَىٰ"
  },
  {
    "id": 7,
    "name": "أَنْ رَآهُ اسْتَغْنَىٰ"
  },
  {
    "id": 8,
    "name": "إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ"
  },
  {
    "id": 9,
    "name": "أَرَأَيْتَ الَّذِي يَنْهَىٰ"
  },
  {
    "id": 10,
    "name": "عَبْدًا إِذَا صَلَّىٰ"
  },
  {
    "id": 11,
    "name": "أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَىٰ"
  },
  {
    "id": 12,
    "name": "أَوْ أَمَرَ بِالتَّقْوَىٰ"
  },
  {
    "id": 13,
    "name": "أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّىٰ"
  },
  {
    "id": 14,
    "name": "أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَىٰ"
  },
  {
    "id": 15,
    "name": "كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ"
  },
  {
    "id": 16,
    "name": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ"
  },
  {
    "id": 17,
    "name": "فَلْيَدْعُ نَادِيَهُ"
  },
  {
    "id": 18,
    "name": "سَنَدْعُ الزَّبَانِيَةَ"
  },
  {
    "id": 19,
    "name": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ ۩"
  }
]