[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ"
  },
  {
    "id": 2,
    "name": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ"
  },
  {
    "id": 3,
    "name": "عَامِلَةٌ نَاصِبَةٌ"
  },
  {
    "id": 4,
    "name": "تَصْلَىٰ نَارًا حَامِيَةً"
  },
  {
    "id": 5,
    "name": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ"
  },
  {
    "id": 6,
    "name": "لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ"
  },
  {
    "id": 7,
    "name": "لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ"
  },
  {
    "id": 8,
    "name": "وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ"
  },
  {
    "id": 9,
    "name": "لِسَعْيِهَا رَاضِيَةٌ"
  },
  {
    "id": 10,
    "name": "فِي جَنَّةٍ عَالِيَةٍ"
  },
  {
    "id": 11,
    "name": "لَا تَسْمَعُ فِيهَا لَاغِيَةً"
  },
  {
    "id": 12,
    "name": "فِيهَا عَيْنٌ جَارِيَةٌ"
  },
  {
    "id": 13,
    "name": "فِيهَا سُرُرٌ مَرْفُوعَةٌ"
  },
  {
    "id": 14,
    "name": "وَأَكْوَابٌ مَوْضُوعَةٌ"
  },
  {
    "id": 15,
    "name": "وَنَمَارِقُ مَصْفُوفَةٌ"
  },
  {
    "id": 16,
    "name": "وَزَرَابِيُّ مَبْثُوثَةٌ"
  },
  {
    "id": 17,
    "name": "أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ"
  },
  {
    "id": 18,
    "name": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ"
  },
  {
    "id": 19,
    "name": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ"
  },
  {
    "id": 20,
    "name": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ"
  },
  {
    "id": 21,
    "name": "فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ"
  },
  {
    "id": 22,
    "name": "لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ"
  },
  {
    "id": 23,
    "name": "إِلَّا مَنْ تَوَلَّىٰ وَكَفَرَ"
  },
  {
    "id": 24,
    "name": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ"
  },
  {
    "id": 25,
    "name": "إِنَّ إِلَيْنَا إِيَابَهُمْ"
  },
  {
    "id": 26,
    "name": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ"
  }
]