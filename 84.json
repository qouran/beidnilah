[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "إِذَا السَّمَاءُ انْشَقَّتْ"
  },
  {
    "id": 2,
    "name": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
  },
  {
    "id": 3,
    "name": "وَإِذَا الْأَرْضُ مُدَّتْ"
  },
  {
    "id": 4,
    "name": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ"
  },
  {
    "id": 5,
    "name": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ"
  },
  {
    "id": 6,
    "name": "يَا أَيُّهَا الْإِنْسَانُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَاقِيهِ"
  },
  {
    "id": 7,
    "name": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ"
  },
  {
    "id": 8,
    "name": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا"
  },
  {
    "id": 9,
    "name": "وَيَنْقَلِبُ إِلَىٰ أَهْلِهِ مَسْرُورًا"
  },
  {
    "id": 10,
    "name": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ وَرَاءَ ظَهْرِهِ"
  },
  {
    "id": 11,
    "name": "فَسَوْفَ يَدْعُو ثُبُورًا"
  },
  {
    "id": 12,
    "name": "وَيَصْلَىٰ سَعِيرًا"
  },
  {
    "id": 13,
    "name": "إِنَّهُ كَانَ فِي أَهْلِهِ مَسْرُورًا"
  },
  {
    "id": 14,
    "name": "إِنَّهُ ظَنَّ أَنْ لَنْ يَحُورَ"
  },
  {
    "id": 15,
    "name": "بَلَىٰ إِنَّ رَبَّهُ كَانَ بِهِ بَصِيرًا"
  },
  {
    "id": 16,
    "name": "فَلَا أُقْسِمُ بِالشَّفَقِ"
  },
  {
    "id": 17,
    "name": "وَاللَّيْلِ وَمَا وَسَقَ"
  },
  {
    "id": 18,
    "name": "وَالْقَمَرِ إِذَا اتَّسَقَ"
  },
  {
    "id": 19,
    "name": "لَتَرْكَبُنَّ طَبَقًا عَنْ طَبَقٍ"
  },
  {
    "id": 20,
    "name": "فَمَا لَهُمْ لَا يُؤْمِنُونَ"
  },
  {
    "id": 21,
    "name": "وَإِذَا قُرِئَ عَلَيْهِمُ الْقُرْآنُ لَا يَسْجُدُونَ ۩"
  },
  {
    "id": 22,
    "name": "بَلِ الَّذِينَ كَفَرُوا يُكَذِّبُونَ"
  },
  {
    "id": 23,
    "name": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَ"
  },
  {
    "id": 24,
    "name": "فَبَشِّرْهُمْ بِعَذَابٍ أَلِيمٍ"
  },
  {
    "id": 25,
    "name": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ"
  }
]
