[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "لَا أُقْسِمُ بِهَٰذَا الْبَلَدِ"
  },
  {
    "id": 2,
    "name": "وَأَنْتَ حِلٌّ بِهَٰذَا الْبَلَدِ"
  },
  {
    "id": 3,
    "name": "وَوَالِدٍ وَمَا وَلَدَ"
  },
  {
    "id": 4,
    "name": "لَقَدْ خَلَقْنَا الْإِنْسَانَ فِي كَبَدٍ"
  },
  {
    "id": 5,
    "name": "أَيَحْسَبُ أَنْ لَنْ يَقْدِرَ عَلَيْهِ أَحَدٌ"
  },
  {
    "id": 6,
    "name": "يَقُولُ أَهْلَكْتُ مَالًا لُبَدًا"
  },
  {
    "id": 7,
    "name": "أَيَحْسَبُ أَنْ لَمْ يَرَهُ أَحَدٌ"
  },
  {
    "id": 8,
    "name": "أَلَمْ نَجْعَلْ لَهُ عَيْنَيْنِ"
  },
  {
    "id": 9,
    "name": "وَلِسَانًا وَشَفَتَيْنِ"
  },
  {
    "id": 10,
    "name": "وَهَدَيْنَاهُ النَّجْدَيْنِ"
  },
  {
    "id": 11,
    "name": "فَلَا اقْتَحَمَ الْعَقَبَةَ"
  },
  {
    "id": 12,
    "name": "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ"
  },
  {
    "id": 13,
    "name": "فَكُّ رَقَبَةٍ"
  },
  {
    "id": 14,
    "name": "أَوْ إِطْعَامٌ فِي يَوْمٍ ذِي مَسْغَبَةٍ"
  },
  {
    "id": 15,
    "name": "يَتِيمًا ذَا مَقْرَبَةٍ"
  },
  {
    "id": 16,
    "name": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ"
  },
  {
    "id": 17,
    "name": "ثُمَّ كَانَ مِنَ الَّذِينَ آمَنُوا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ"
  },
  {
    "id": 18,
    "name": "أُولَٰئِكَ أَصْحَابُ الْمَيْمَنَةِ"
  },
  {
    "id": 19,
    "name": "وَالَّذِينَ كَفَرُوا بِآيَاتِنَا هُمْ أَصْحَابُ الْمَشْأَمَةِ"
  },
  {
    "id": 20,
    "name": "عَلَيْهِمْ نَارٌ مُؤْصَدَةٌ"
  }
]