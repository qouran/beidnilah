[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "عَمَّ يَتَسَاءَلُونَ"
  },
  {
    "id": 2,
    "name": "عَنِ النَّبَإِ الْعَظِيمِ"
  },
  {
    "id": 3,
    "name": "الَّذِي هُمْ فِيهِ مُخْتَلِفُونَ"
  },
  {
    "id": 4,
    "name": "كَلَّا سَيَعْلَمُونَ"
  },
  {
    "id": 5,
    "name": "ثُمَّ كَلَّا سَيَعْلَمُونَ"
  },
  {
    "id": 6,
    "name": "أَلَمْ نَجْعَلِ الْأَرْضَ مِهَادًا"
  },
  {
    "id": 7,
    "name": "وَالْجِبَالَ أَوْتَادًا"
  },
  {
    "id": 8,
    "name": "وَخَلَقْنَاكُمْ أَزْوَاجًا"
  },
  {
    "id": 9,
    "name": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا"
  },
  {
    "id": 10,
    "name": "وَجَعَلْنَا اللَّيْلَ لِبَاسًا"
  },
  {
    "id": 11,
    "name": "وَجَعَلْنَا النَّهَارَ مَعَاشًا"
  },
  {
    "id": 12,
    "name": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا"
  },
  {
    "id": 13,
    "name": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا"
  },
  {
    "id": 14,
    "name": "وَأَنْزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا"
  },
  {
    "id": 15,
    "name": "لِنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا"
  },
  {
    "id": 16,
    "name": "وَجَنَّاتٍ أَلْفَافًا"
  },
  {
    "id": 17,
    "name": "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا"
  },
  {
    "id": 18,
    "name": "يَوْمَ يُنْفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا"
  },
  {
    "id": 19,
    "name": "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا"
  },
  {
    "id": 20,
    "name": "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا"
  },
  {
    "id": 21,
    "name": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا"
  },
  {
    "id": 22,
    "name": "لِلطَّاغِينَ مَآبًا"
  },
  {
    "id": 23,
    "name": "لَابِثِينَ فِيهَا أَحْقَابًا"
  },
  {
    "id": 24,
    "name": "لَا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا"
  },
  {
    "id": 25,
    "name": "إِلَّا حَمِيمًا وَغَسَّاقًا"
  },
  {
    "id": 26,
    "name": "جَزَاءً وِفَاقًا"
  },
  {
    "id": 27,
    "name": "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا"
  },
  {
    "id": 28,
    "name": "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا"
  },
  {
    "id": 29,
    "name": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا"
  },
  {
    "id": 30,
    "name": "فَذُوقُوا فَلَنْ نَزِيدَكُمْ إِلَّا عَذَابًا"
  },
  {
    "id": 31,
    "name": "إِنَّ لِلْمُتَّقِينَ مَفَازًا"
  },
  {
    "id": 32,
    "name": "حَدَائِقَ وَأَعْنَابًا"
  },
  {
    "id": 33,
    "name": "وَكَوَاعِبَ أَتْرَابًا"
  },
  {
    "id": 34,
    "name": "وَكَأْسًا دِهَاقًا"
  },
  {
    "id": 35,
    "name": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا"
  },
  {
    "id": 36,
    "name": "جَزَاءً مِنْ رَبِّكَ عَطَاءً حِسَابًا"
  },
  {
    "id": 37,
    "name": "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا"
  },
  {
    "id": 38,
    "name": "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا ۖ لَا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَٰنُ وَقَالَ صَوَابًا"
  },
  {
    "id": 39,
    "name": "ذَٰلِكَ الْيَوْمُ الْحَقُّ ۖ فَمَنْ شَاءَ اتَّخَذَ إِلَىٰ رَبِّهِ مَآبًا"
  },
  {
    "id": 40,
    "name": "إِنَّا أَنْذَرْنَاكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ الْكَافِرُ يَا لَيْتَنِي كُنْتُ تُرَابًا"
  }
]