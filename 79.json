[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالنَّازِعَاتِ غَرْقًا"
  },
  {
    "id": 2,
    "name": "وَالنَّاشِطَاتِ نَشْطًا"
  },
  {
    "id": 3,
    "name": "وَالسَّابِحَاتِ سَبْحًا"
  },
  {
    "id": 4,
    "name": "فَالسَّابِقَاتِ سَبْقًا"
  },
  {
    "id": 5,
    "name": "فَالْمُدَبِّرَاتِ أَمْرًا"
  },
  {
    "id": 6,
    "name": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ"
  },
  {
    "id": 7,
    "name": "تَتْبَعُهَا الرَّادِفَةُ"
  },
  {
    "id": 8,
    "name": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ"
  },
  {
    "id": 9,
    "name": "أَبْصَارُهَا خَاشِعَةٌ"
  },
  {
    "id": 10,
    "name": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ"
  },
  {
    "id": 11,
    "name": "أَإِذَا كُنَّا عِظَامًا نَخِرَةً"
  },
  {
    "id": 12,
    "name": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ"
  },
  {
    "id": 13,
    "name": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ"
  },
  {
    "id": 14,
    "name": "فَإِذَا هُمْ بِالسَّاهِرَةِ"
  },
  {
    "id": 15,
    "name": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ"
  },
  {
    "id": 16,
    "name": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى"
  },
  {
    "id": 17,
    "name": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ"
  },
  {
    "id": 18,
    "name": "فَقُلْ هَلْ لَكَ إِلَىٰ أَنْ تَزَكَّىٰ"
  },
  {
    "id": 19,
    "name": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ"
  },
  {
    "id": 20,
    "name": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ"
  },
  {
    "id": 21,
    "name": "فَكَذَّبَ وَعَصَىٰ"
  },
  {
    "id": 22,
    "name": "ثُمَّ أَدْبَرَ يَسْعَىٰ"
  },
  {
    "id": 23,
    "name": "فَحَشَرَ فَنَادَىٰ"
  },
  {
    "id": 24,
    "name": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ"
  },
  {
    "id": 25,
    "name": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ"
  },
  {
    "id": 26,
    "name": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِمَنْ يَخْشَىٰ"
  },
  {
    "id": 27,
    "name": "أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا"
  },
  {
    "id": 28,
    "name": "رَفَعَ سَمْكَهَا فَسَوَّاهَا"
  },
  {
    "id": 29,
    "name": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا"
  },
  {
    "id": 30,
    "name": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا"
  },
  {
    "id": 31,
    "name": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا"
  },
  {
    "id": 32,
    "name": "وَالْجِبَالَ أَرْسَاهَا"
  },
  {
    "id": 33,
    "name": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ"
  },
  {
    "id": 34,
    "name": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ"
  },
  {
    "id": 35,
    "name": "يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَىٰ"
  },
  {
    "id": 36,
    "name": "وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَىٰ"
  },
  {
    "id": 37,
    "name": "فَأَمَّا مَنْ طَغَىٰ"
  },
  {
    "id": 38,
    "name": "وَآثَرَ الْحَيَاةَ الدُّنْيَا"
  },
  {
    "id": 39,
    "name": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ"
  },
  {
    "id": 40,
    "name": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ"
  },
  {
    "id": 41,
    "name": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ"
  },
  {
    "id": 42,
    "name": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا"
  },
  {
    "id": 43,
    "name": "فِيمَ أَنْتَ مِنْ ذِكْرَاهَا"
  },
  {
    "id": 44,
    "name": "إِلَىٰ رَبِّكَ مُنْتَهَاهَا"
  },
  {
    "id": 45,
    "name": "إِنَّمَا أَنْتَ مُنْذِرُ مَنْ يَخْشَاهَا"
  },
  {
    "id": 46,
    "name": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا"
  }
]