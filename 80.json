[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "عَبَسَ وَتَوَلَّىٰ"
  },
  {
    "id": 2,
    "name": "أَنْ جَاءَهُ الْأَعْمَىٰ"
  },
  {
    "id": 3,
    "name": "وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ"
  },
  {
    "id": 4,
    "name": "أَوْ يَذَّكَّرُ فَتَنْفَعَهُ الذِّكْرَىٰ"
  },
  {
    "id": 5,
    "name": "أَمَّا مَنِ اسْتَغْنَىٰ"
  },
  {
    "id": 6,
    "name": "فَأَنْتَ لَهُ تَصَدَّىٰ"
  },
  {
    "id": 7,
    "name": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ"
  },
  {
    "id": 8,
    "name": "وَأَمَّا مَنْ جَاءَكَ يَسْعَىٰ"
  },
  {
    "id": 9,
    "name": "وَهُوَ يَخْشَىٰ"
  },
  {
    "id": 10,
    "name": "فَأَنْتَ عَنْهُ تَلَهَّىٰ"
  },
  {
    "id": 11,
    "name": "كَلَّا إِنَّهَا تَذْكِرَةٌ"
  },
  {
    "id": 12,
    "name": "فَمَنْ شَاءَ ذَكَرَهُ"
  },
  {
    "id": 13,
    "name": "فِي صُحُفٍ مُكَرَّمَةٍ"
  },
  {
    "id": 14,
    "name": "مَرْفُوعَةٍ مُطَهَّرَةٍ"
  },
  {
    "id": 15,
    "name": "بِأَيْدِي سَفَرَةٍ"
  },
  {
    "id": 16,
    "name": "كِرَامٍ بَرَرَةٍ"
  },
  {
    "id": 17,
    "name": "قُتِلَ الْإِنْسَانُ مَا أَكْفَرَهُ"
  },
  {
    "id": 18,
    "name": "مِنْ أَيِّ شَيْءٍ خَلَقَهُ"
  },
  {
    "id": 19,
    "name": "مِنْ نُطْفَةٍ خَلَقَهُ فَقَدَّرَهُ"
  },
  {
    "id": 20,
    "name": "ثُمَّ السَّبِيلَ يَسَّرَهُ"
  },
  {
    "id": 21,
    "name": "ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ"
  },
  {
    "id": 22,
    "name": "ثُمَّ إِذَا شَاءَ أَنْشَرَهُ"
  },
  {
    "id": 23,
    "name": "كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ"
  },
  {
    "id": 24,
    "name": "فَلْيَنْظُرِ الْإِنْسَانُ إِلَىٰ طَعَامِهِ"
  },
  {
    "id": 25,
    "name": "أَنَّا صَبَبْنَا الْمَاءَ صَبًّا"
  },
  {
    "id": 26,
    "name": "ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا"
  },
  {
    "id": 27,
    "name": "فَأَنْبَتْنَا فِيهَا حَبًّا"
  },
  {
    "id": 28,
    "name": "وَعِنَبًا وَقَضْبًا"
  },
  {
    "id": 29,
    "name": "وَزَيْتُونًا وَنَخْلًا"
  },
  {
    "id": 30,
    "name": "وَحَدَائِقَ غُلْبًا"
  },
  {
    "id": 31,
    "name": "وَفَاكِهَةً وَأَبًّا"
  },
  {
    "id": 32,
    "name": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ"
  },
  {
    "id": 33,
    "name": "فَإِذَا جَاءَتِ الصَّاخَّةُ"
  },
  {
    "id": 34,
    "name": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ"
  },
  {
    "id": 35,
    "name": "وَأُمِّهِ وَأَبِيهِ"
  },
  {
    "id": 36,
    "name": "وَصَاحِبَتِهِ وَبَنِيهِ"
  },
  {
    "id": 37,
    "name": "لِكُلِّ امْرِئٍ مِنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ"
  },
  {
    "id": 38,
    "name": "وُجُوهٌ يَوْمَئِذٍ مُسْفِرَةٌ"
  },
  {
    "id": 39,
    "name": "ضَاحِكَةٌ مُسْتَبْشِرَةٌ"
  },
  {
    "id": 40,
    "name": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ"
  },
  {
    "id": 41,
    "name": "تَرْهَقُهَا قَتَرَةٌ"
  },
  {
    "id": 42,
    "name": "أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ"
  }
]