[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَيْلٌ لِلْمُطَفِّفِينَ"
  },
  {
    "id": 2,
    "name": "الَّذِينَ إِذَا اكْتَالُوا عَلَى النَّاسِ يَسْتَوْفُونَ"
  },
  {
    "id": 3,
    "name": "وَإِذَا كَالُوهُمْ أَوْ وَزَنُوهُمْ يُخْسِرُونَ"
  },
  {
    "id": 4,
    "name": "أَلَا يَظُنُّ أُولَٰئِكَ أَنَّهُمْ مَبْعُوثُونَ"
  },
  {
    "id": 5,
    "name": "لِيَوْمٍ عَظِيمٍ"
  },
  {
    "id": 6,
    "name": "يَوْمَ يَقُومُ النَّاسُ لِرَبِّ الْعَالَمِينَ"
  },
  {
    "id": 7,
    "name": "كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ"
  },
  {
    "id": 8,
    "name": "وَمَا أَدْرَاكَ مَا سِجِّينٌ"
  },
  {
    "id": 9,
    "name": "كِتَابٌ مَرْقُومٌ"
  },
  {
    "id": 10,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 11,
    "name": "الَّذِينَ يُكَذِّبُونَ بِيَوْمِ الدِّينِ"
  },
  {
    "id": 12,
    "name": "وَمَا يُكَذِّبُ بِهِ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ"
  },
  {
    "id": 13,
    "name": "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ"
  },
  {
    "id": 14,
    "name": "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِمْ مَا كَانُوا يَكْسِبُونَ"
  },
  {
    "id": 15,
    "name": "كَلَّا إِنَّهُمْ عَنْ رَبِّهِمْ يَوْمَئِذٍ لَمَحْجُوبُونَ"
  },
  {
    "id": 16,
    "name": "ثُمَّ إِنَّهُمْ لَصَالُو الْجَحِيمِ"
  },
  {
    "id": 17,
    "name": "ثُمَّ يُقَالُ هَٰذَا الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ"
  },
  {
    "id": 18,
    "name": "كَلَّا إِنَّ كِتَابَ الْأَبْرَارِ لَفِي عِلِّيِّينَ"
  },
  {
    "id": 19,
    "name": "وَمَا أَدْرَاكَ مَا عِلِّيُّونَ"
  },
  {
    "id": 20,
    "name": "كِتَابٌ مَرْقُومٌ"
  },
  {
    "id": 21,
    "name": "يَشْهَدُهُ الْمُقَرَّبُونَ"
  },
  {
    "id": 22,
    "name": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
  },
  {
    "id": 23,
    "name": "عَلَى الْأَرَائِكِ يَنْظُرُونَ"
  },
  {
    "id": 24,
    "name": "تَعْرِفُ فِي وُجُوهِهِمْ نَضْرَةَ النَّعِيمِ"
  },
  {
    "id": 25,
    "name": "يُسْقَوْنَ مِنْ رَحِيقٍ مَخْتُومٍ"
  },
  {
    "id": 26,
    "name": "خِتَامُهُ مِسْكٌ ۚ وَفِي ذَٰلِكَ فَلْيَتَنَافَسِ الْمُتَنَافِسُونَ"
  },
  {
    "id": 27,
    "name": "وَمِزَاجُهُ مِنْ تَسْنِيمٍ"
  },
  {
    "id": 28,
    "name": "عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ"
  },
  {
    "id": 29,
    "name": "إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ الَّذِينَ آمَنُوا يَضْحَكُونَ"
  },
  {
    "id": 30,
    "name": "وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ"
  },
  {
    "id": 31,
    "name": "وَإِذَا انْقَلَبُوا إِلَىٰ أَهْلِهِمُ انْقَلَبُوا فَكِهِينَ"
  },
  {
    "id": 32,
    "name": "وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَٰؤُلَاءِ لَضَالُّونَ"
  },
  {
    "id": 33,
    "name": "وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ"
  },
  {
    "id": 34,
    "name": "فَالْيَوْمَ الَّذِينَ آمَنُوا مِنَ الْكُفَّارِ يَضْحَكُونَ"
  },
  {
    "id": 35,
    "name": "عَلَى الْأَرَائِكِ يَنْظُرُونَ"
  },
  {
    "id": 36,
    "name": "هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوا يَفْعَلُونَ"
  }
]