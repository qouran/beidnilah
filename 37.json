[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالصَّافَّاتِ صَفًّا"
  },
  {
    "id": 2,
    "name": "فَالزَّاجِرَاتِ زَجْرًا"
  },
  {
    "id": 3,
    "name": "فَالتَّالِيَاتِ ذِكْرًا"
  },
  {
    "id": 4,
    "name": "إِنَّ إِلَٰهَكُمْ لَوَاحِدٌ"
  },
  {
    "id": 5,
    "name": "رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ"
  },
  {
    "id": 6,
    "name": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ"
  },
  {
    "id": 7,
    "name": "وَحِفْظًا مِنْ كُلِّ شَيْطَانٍ مَارِدٍ"
  },
  {
    "id": 8,
    "name": "لَا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِنْ كُلِّ جَانِبٍ"
  },
  {
    "id": 9,
    "name": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ"
  },
  {
    "id": 10,
    "name": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ"
  },
  {
    "id": 11,
    "name": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَمْ مَنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُمْ مِنْ طِينٍ لَازِبٍ"
  },
  {
    "id": 12,
    "name": "بَلْ عَجِبْتَ وَيَسْخَرُونَ"
  },
  {
    "id": 13,
    "name": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ"
  },
  {
    "id": 14,
    "name": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ"
  },
  {
    "id": 15,
    "name": "وَقَالُوا إِنْ هَٰذَا إِلَّا سِحْرٌ مُبِينٌ"
  },
  {
    "id": 16,
    "name": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
  },
  {
    "id": 17,
    "name": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
  },
  {
    "id": 18,
    "name": "قُلْ نَعَمْ وَأَنْتُمْ دَاخِرُونَ"
  },
  {
    "id": 19,
    "name": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنْظُرُونَ"
  },
  {
    "id": 20,
    "name": "وَقَالُوا يَا وَيْلَنَا هَٰذَا يَوْمُ الدِّينِ"
  },
  {
    "id": 21,
    "name": "هَٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ"
  },
  {
    "id": 22,
    "name": "۞ احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ"
  },
  {
    "id": 23,
    "name": "مِنْ دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ"
  },
  {
    "id": 24,
    "name": "وَقِفُوهُمْ ۖ إِنَّهُمْ مَسْئُولُونَ"
  },
  {
    "id": 25,
    "name": "مَا لَكُمْ لَا تَنَاصَرُونَ"
  },
  {
    "id": 26,
    "name": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ"
  },
  {
    "id": 27,
    "name": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
  },
  {
    "id": 28,
    "name": "قَالُوا إِنَّكُمْ كُنْتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ"
  },
  {
    "id": 29,
    "name": "قَالُوا بَلْ لَمْ تَكُونُوا مُؤْمِنِينَ"
  },
  {
    "id": 30,
    "name": "وَمَا كَانَ لَنَا عَلَيْكُمْ مِنْ سُلْطَانٍ ۖ بَلْ كُنْتُمْ قَوْمًا طَاغِينَ"
  },
  {
    "id": 31,
    "name": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ"
  },
  {
    "id": 32,
    "name": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ"
  },
  {
    "id": 33,
    "name": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ"
  },
  {
    "id": 34,
    "name": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
  },
  {
    "id": 35,
    "name": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ"
  },
  {
    "id": 36,
    "name": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَجْنُونٍ"
  },
  {
    "id": 37,
    "name": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ"
  },
  {
    "id": 38,
    "name": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ"
  },
  {
    "id": 39,
    "name": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 40,
    "name": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
  },
  {
    "id": 41,
    "name": "أُولَٰئِكَ لَهُمْ رِزْقٌ مَعْلُومٌ"
  },
  {
    "id": 42,
    "name": "فَوَاكِهُ ۖ وَهُمْ مُكْرَمُونَ"
  },
  {
    "id": 43,
    "name": "فِي جَنَّاتِ النَّعِيمِ"
  },
  {
    "id": 44,
    "name": "عَلَىٰ سُرُرٍ مُتَقَابِلِينَ"
  },
  {
    "id": 45,
    "name": "يُطَافُ عَلَيْهِمْ بِكَأْسٍ مِنْ مَعِينٍ"
  },
  {
    "id": 46,
    "name": "بَيْضَاءَ لَذَّةٍ لِلشَّارِبِينَ"
  },
  {
    "id": 47,
    "name": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنْزَفُونَ"
  },
  {
    "id": 48,
    "name": "وَعِنْدَهُمْ قَاصِرَاتُ الطَّرْفِ عِينٌ"
  },
  {
    "id": 49,
    "name": "كَأَنَّهُنَّ بَيْضٌ مَكْنُونٌ"
  },
  {
    "id": 50,
    "name": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ"
  },
  {
    "id": 51,
    "name": "قَالَ قَائِلٌ مِنْهُمْ إِنِّي كَانَ لِي قَرِينٌ"
  },
  {
    "id": 52,
    "name": "يَقُولُ أَإِنَّكَ لَمِنَ الْمُصَدِّقِينَ"
  },
  {
    "id": 53,
    "name": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَدِينُونَ"
  },
  {
    "id": 54,
    "name": "قَالَ هَلْ أَنْتُمْ مُطَّلِعُونَ"
  },
  {
    "id": 55,
    "name": "فَاطَّلَعَ فَرَآهُ فِي سَوَاءِ الْجَحِيمِ"
  },
  {
    "id": 56,
    "name": "قَالَ تَاللَّهِ إِنْ كِدْتَ لَتُرْدِينِ"
  },
  {
    "id": 57,
    "name": "وَلَوْلَا نِعْمَةُ رَبِّي لَكُنْتُ مِنَ الْمُحْضَرِينَ"
  },
  {
    "id": 58,
    "name": "أَفَمَا نَحْنُ بِمَيِّتِينَ"
  },
  {
    "id": 59,
    "name": "إِلَّا مَوْتَتَنَا الْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ"
  },
  {
    "id": 60,
    "name": "إِنَّ هَٰذَا لَهُوَ الْفَوْزُ الْعَظِيمُ"
  },
  {
    "id": 61,
    "name": "لِمِثْلِ هَٰذَا فَلْيَعْمَلِ الْعَامِلُونَ"
  },
  {
    "id": 62,
    "name": "أَذَٰلِكَ خَيْرٌ نُزُلًا أَمْ شَجَرَةُ الزَّقُّومِ"
  },
  {
    "id": 63,
    "name": "إِنَّا جَعَلْنَاهَا فِتْنَةً لِلظَّالِمِينَ"
  },
  {
    "id": 64,
    "name": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِي أَصْلِ الْجَحِيمِ"
  },
  {
    "id": 65,
    "name": "طَلْعُهَا كَأَنَّهُ رُءُوسُ الشَّيَاطِينِ"
  },
  {
    "id": 66,
    "name": "فَإِنَّهُمْ لَآكِلُونَ مِنْهَا فَمَالِئُونَ مِنْهَا الْبُطُونَ"
  },
  {
    "id": 67,
    "name": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِنْ حَمِيمٍ"
  },
  {
    "id": 68,
    "name": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى الْجَحِيمِ"
  },
  {
    "id": 69,
    "name": "إِنَّهُمْ أَلْفَوْا آبَاءَهُمْ ضَالِّينَ"
  },
  {
    "id": 70,
    "name": "فَهُمْ عَلَىٰ آثَارِهِمْ يُهْرَعُونَ"
  },
  {
    "id": 71,
    "name": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ الْأَوَّلِينَ"
  },
  {
    "id": 72,
    "name": "وَلَقَدْ أَرْسَلْنَا فِيهِمْ مُنْذِرِينَ"
  },
  {
    "id": 73,
    "name": "فَانْظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُنْذَرِينَ"
  },
  {
    "id": 74,
    "name": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
  },
  {
    "id": 75,
    "name": "وَلَقَدْ نَادَانَا نُوحٌ فَلَنِعْمَ الْمُجِيبُونَ"
  },
  {
    "id": 76,
    "name": "وَنَجَّيْنَاهُ وَأَهْلَهُ مِنَ الْكَرْبِ الْعَظِيمِ"
  },
  {
    "id": 77,
    "name": "وَجَعَلْنَا ذُرِّيَّتَهُ هُمُ الْبَاقِينَ"
  },
  {
    "id": 78,
    "name": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
  },
  {
    "id": 79,
    "name": "سَلَامٌ عَلَىٰ نُوحٍ فِي الْعَالَمِينَ"
  },
  {
    "id": 80,
    "name": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
  },
  {
    "id": 81,
    "name": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
  },
  {
    "id": 82,
    "name": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ"
  },
  {
    "id": 83,
    "name": "۞ وَإِنَّ مِنْ شِيعَتِهِ لَإِبْرَاهِيمَ"
  },
  {
    "id": 84,
    "name": "إِذْ جَاءَ رَبَّهُ بِقَلْبٍ سَلِيمٍ"
  },
  {
    "id": 85,
    "name": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَاذَا تَعْبُدُونَ"
  },
  {
    "id": 86,
    "name": "أَئِفْكًا آلِهَةً دُونَ اللَّهِ تُرِيدُونَ"
  },
  {
    "id": 87,
    "name": "فَمَا ظَنُّكُمْ بِرَبِّ الْعَالَمِينَ"
  },
  {
    "id": 88,
    "name": "فَنَظَرَ نَظْرَةً فِي النُّجُومِ"
  },
  {
    "id": 89,
    "name": "فَقَالَ إِنِّي سَقِيمٌ"
  },
  {
    "id": 90,
    "name": "فَتَوَلَّوْا عَنْهُ مُدْبِرِينَ"
  },
  {
    "id": 91,
    "name": "فَرَاغَ إِلَىٰ آلِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ"
  },
  {
    "id": 92,
    "name": "مَا لَكُمْ لَا تَنْطِقُونَ"
  },
  {
    "id": 93,
    "name": "فَرَاغَ عَلَيْهِمْ ضَرْبًا بِالْيَمِينِ"
  },
  {
    "id": 94,
    "name": "فَأَقْبَلُوا إِلَيْهِ يَزِفُّونَ"
  },
  {
    "id": 95,
    "name": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ"
  },
  {
    "id": 96,
    "name": "وَاللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ"
  },
  {
    "id": 97,
    "name": "قَالُوا ابْنُوا لَهُ بُنْيَانًا فَأَلْقُوهُ فِي الْجَحِيمِ"
  },
  {
    "id": 98,
    "name": "فَأَرَادُوا بِهِ كَيْدًا فَجَعَلْنَاهُمُ الْأَسْفَلِينَ"
  },
  {
    "id": 99,
    "name": "وَقَالَ إِنِّي ذَاهِبٌ إِلَىٰ رَبِّي سَيَهْدِينِ"
  },
  {
    "id": 100,
    "name": "رَبِّ هَبْ لِي مِنَ الصَّالِحِينَ"
  },
  {
    "id": 101,
    "name": "فَبَشَّرْنَاهُ بِغُلَامٍ حَلِيمٍ"
  },
  {
    "id": 102,
    "name": "فَلَمَّا بَلَغَ مَعَهُ السَّعْيَ قَالَ يَا بُنَيَّ إِنِّي أَرَىٰ فِي الْمَنَامِ أَنِّي أَذْبَحُكَ فَانْظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَا أَبَتِ افْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِي إِنْ شَاءَ اللَّهُ مِنَ الصَّابِرِينَ"
  },
  {
    "id": 103,
    "name": "فَلَمَّا أَسْلَمَا وَتَلَّهُ لِلْجَبِينِ"
  },
  {
    "id": 104,
    "name": "وَنَادَيْنَاهُ أَنْ يَا إِبْرَاهِيمُ"
  },
  {
    "id": 105,
    "name": "قَدْ صَدَّقْتَ الرُّؤْيَا ۚ إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
  },
  {
    "id": 106,
    "name": "إِنَّ هَٰذَا لَهُوَ الْبَلَاءُ الْمُبِينُ"
  },
  {
    "id": 107,
    "name": "وَفَدَيْنَاهُ بِذِبْحٍ عَظِيمٍ"
  },
  {
    "id": 108,
    "name": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
  },
  {
    "id": 109,
    "name": "سَلَامٌ عَلَىٰ إِبْرَاهِيمَ"
  },
  {
    "id": 110,
    "name": "كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
  },
  {
    "id": 111,
    "name": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
  },
  {
    "id": 112,
    "name": "وَبَشَّرْنَاهُ بِإِسْحَاقَ نَبِيًّا مِنَ الصَّالِحِينَ"
  },
  {
    "id": 113,
    "name": "وَبَارَكْنَا عَلَيْهِ وَعَلَىٰ إِسْحَاقَ ۚ وَمِنْ ذُرِّيَّتِهِمَا مُحْسِنٌ وَظَالِمٌ لِنَفْسِهِ مُبِينٌ"
  },
  {
    "id": 114,
    "name": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَارُونَ"
  },
  {
    "id": 115,
    "name": "وَنَجَّيْنَاهُمَا وَقَوْمَهُمَا مِنَ الْكَرْبِ الْعَظِيمِ"
  },
  {
    "id": 116,
    "name": "وَنَصَرْنَاهُمْ فَكَانُوا هُمُ الْغَالِبِينَ"
  },
  {
    "id": 117,
    "name": "وَآتَيْنَاهُمَا الْكِتَابَ الْمُسْتَبِينَ"
  },
  {
    "id": 118,
    "name": "وَهَدَيْنَاهُمَا الصِّرَاطَ الْمُسْتَقِيمَ"
  },
  {
    "id": 119,
    "name": "وَتَرَكْنَا عَلَيْهِمَا فِي الْآخِرِينَ"
  },
  {
    "id": 120,
    "name": "سَلَامٌ عَلَىٰ مُوسَىٰ وَهَارُونَ"
  },
  {
    "id": 121,
    "name": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
  },
  {
    "id": 122,
    "name": "إِنَّهُمَا مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
  },
  {
    "id": 123,
    "name": "وَإِنَّ إِلْيَاسَ لَمِنَ الْمُرْسَلِينَ"
  },
  {
    "id": 124,
    "name": "إِذْ قَالَ لِقَوْمِهِ أَلَا تَتَّقُونَ"
  },
  {
    "id": 125,
    "name": "أَتَدْعُونَ بَعْلًا وَتَذَرُونَ أَحْسَنَ الْخَالِقِينَ"
  },
  {
    "id": 126,
    "name": "اللَّهَ رَبَّكُمْ وَرَبَّ آبَائِكُمُ الْأَوَّلِينَ"
  },
  {
    "id": 127,
    "name": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ"
  },
  {
    "id": 128,
    "name": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
  },
  {
    "id": 129,
    "name": "وَتَرَكْنَا عَلَيْهِ فِي الْآخِرِينَ"
  },
  {
    "id": 130,
    "name": "سَلَامٌ عَلَىٰ إِلْ يَاسِينَ"
  },
  {
    "id": 131,
    "name": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
  },
  {
    "id": 132,
    "name": "إِنَّهُ مِنْ عِبَادِنَا الْمُؤْمِنِينَ"
  },
  {
    "id": 133,
    "name": "وَإِنَّ لُوطًا لَمِنَ الْمُرْسَلِينَ"
  },
  {
    "id": 134,
    "name": "إِذْ نَجَّيْنَاهُ وَأَهْلَهُ أَجْمَعِينَ"
  },
  {
    "id": 135,
    "name": "إِلَّا عَجُوزًا فِي الْغَابِرِينَ"
  },
  {
    "id": 136,
    "name": "ثُمَّ دَمَّرْنَا الْآخَرِينَ"
  },
  {
    "id": 137,
    "name": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِمْ مُصْبِحِينَ"
  },
  {
    "id": 138,
    "name": "وَبِاللَّيْلِ ۗ أَفَلَا تَعْقِلُونَ"
  },
  {
    "id": 139,
    "name": "وَإِنَّ يُونُسَ لَمِنَ الْمُرْسَلِينَ"
  },
  {
    "id": 140,
    "name": "إِذْ أَبَقَ إِلَى الْفُلْكِ الْمَشْحُونِ"
  },
  {
    "id": 141,
    "name": "فَسَاهَمَ فَكَانَ مِنَ الْمُدْحَضِينَ"
  },
  {
    "id": 142,
    "name": "فَالْتَقَمَهُ الْحُوتُ وَهُوَ مُلِيمٌ"
  },
  {
    "id": 143,
    "name": "فَلَوْلَا أَنَّهُ كَانَ مِنَ الْمُسَبِّحِينَ"
  },
  {
    "id": 144,
    "name": "لَلَبِثَ فِي بَطْنِهِ إِلَىٰ يَوْمِ يُبْعَثُونَ"
  },
  {
    "id": 145,
    "name": "۞ فَنَبَذْنَاهُ بِالْعَرَاءِ وَهُوَ سَقِيمٌ"
  },
  {
    "id": 146,
    "name": "وَأَنْبَتْنَا عَلَيْهِ شَجَرَةً مِنْ يَقْطِينٍ"
  },
  {
    "id": 147,
    "name": "وَأَرْسَلْنَاهُ إِلَىٰ مِائَةِ أَلْفٍ أَوْ يَزِيدُونَ"
  },
  {
    "id": 148,
    "name": "فَآمَنُوا فَمَتَّعْنَاهُمْ إِلَىٰ حِينٍ"
  },
  {
    "id": 149,
    "name": "فَاسْتَفْتِهِمْ أَلِرَبِّكَ الْبَنَاتُ وَلَهُمُ الْبَنُونَ"
  },
  {
    "id": 150,
    "name": "أَمْ خَلَقْنَا الْمَلَائِكَةَ إِنَاثًا وَهُمْ شَاهِدُونَ"
  },
  {
    "id": 151,
    "name": "أَلَا إِنَّهُمْ مِنْ إِفْكِهِمْ لَيَقُولُونَ"
  },
  {
    "id": 152,
    "name": "وَلَدَ اللَّهُ وَإِنَّهُمْ لَكَاذِبُونَ"
  },
  {
    "id": 153,
    "name": "أَصْطَفَى الْبَنَاتِ عَلَى الْبَنِينَ"
  },
  {
    "id": 154,
    "name": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ"
  },
  {
    "id": 155,
    "name": "أَفَلَا تَذَكَّرُونَ"
  },
  {
    "id": 156,
    "name": "أَمْ لَكُمْ سُلْطَانٌ مُبِينٌ"
  },
  {
    "id": 157,
    "name": "فَأْتُوا بِكِتَابِكُمْ إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 158,
    "name": "وَجَعَلُوا بَيْنَهُ وَبَيْنَ الْجِنَّةِ نَسَبًا ۚ وَلَقَدْ عَلِمَتِ الْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَ"
  },
  {
    "id": 159,
    "name": "سُبْحَانَ اللَّهِ عَمَّا يَصِفُونَ"
  },
  {
    "id": 160,
    "name": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
  },
  {
    "id": 161,
    "name": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ"
  },
  {
    "id": 162,
    "name": "مَا أَنْتُمْ عَلَيْهِ بِفَاتِنِينَ"
  },
  {
    "id": 163,
    "name": "إِلَّا مَنْ هُوَ صَالِ الْجَحِيمِ"
  },
  {
    "id": 164,
    "name": "وَمَا مِنَّا إِلَّا لَهُ مَقَامٌ مَعْلُومٌ"
  },
  {
    "id": 165,
    "name": "وَإِنَّا لَنَحْنُ الصَّافُّونَ"
  },
  {
    "id": 166,
    "name": "وَإِنَّا لَنَحْنُ الْمُسَبِّحُونَ"
  },
  {
    "id": 167,
    "name": "وَإِنْ كَانُوا لَيَقُولُونَ"
  },
  {
    "id": 168,
    "name": "لَوْ أَنَّ عِنْدَنَا ذِكْرًا مِنَ الْأَوَّلِينَ"
  },
  {
    "id": 169,
    "name": "لَكُنَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ"
  },
  {
    "id": 170,
    "name": "فَكَفَرُوا بِهِ ۖ فَسَوْفَ يَعْلَمُونَ"
  },
  {
    "id": 171,
    "name": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا الْمُرْسَلِينَ"
  },
  {
    "id": 172,
    "name": "إِنَّهُمْ لَهُمُ الْمَنْصُورُونَ"
  },
  {
    "id": 173,
    "name": "وَإِنَّ جُنْدَنَا لَهُمُ الْغَالِبُونَ"
  },
  {
    "id": 174,
    "name": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
  },
  {
    "id": 175,
    "name": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَ"
  },
  {
    "id": 176,
    "name": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
  },
  {
    "id": 177,
    "name": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَاءَ صَبَاحُ الْمُنْذَرِينَ"
  },
  {
    "id": 178,
    "name": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ"
  },
  {
    "id": 179,
    "name": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ"
  },
  {
    "id": 180,
    "name": "سُبْحَانَ رَبِّكَ رَبِّ الْعِزَّةِ عَمَّا يَصِفُونَ"
  },
  {
    "id": 181,
    "name": "وَسَلَامٌ عَلَى الْمُرْسَلِينَ"
  },
  {
    "id": 182,
    "name": "وَالْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ"
  }
]