[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "إِذَا الشَّمْسُ كُوِّرَتْ"
  },
  {
    "id": 2,
    "name": "وَإِذَا النُّجُومُ انْكَدَرَتْ"
  },
  {
    "id": 3,
    "name": "وَإِذَا الْجِبَالُ سُيِّرَتْ"
  },
  {
    "id": 4,
    "name": "وَإِذَا الْعِشَارُ عُطِّلَتْ"
  },
  {
    "id": 5,
    "name": "وَإِذَا الْوُحُوشُ حُشِرَتْ"
  },
  {
    "id": 6,
    "name": "وَإِذَا الْبِحَارُ سُجِّرَتْ"
  },
  {
    "id": 7,
    "name": "وَإِذَا النُّفُوسُ زُوِّجَتْ"
  },
  {
    "id": 8,
    "name": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ"
  },
  {
    "id": 9,
    "name": "بِأَيِّ ذَنْبٍ قُتِلَتْ"
  },
  {
    "id": 10,
    "name": "وَإِذَا الصُّحُفُ نُشِرَتْ"
  },
  {
    "id": 11,
    "name": "وَإِذَا السَّمَاءُ كُشِطَتْ"
  },
  {
    "id": 12,
    "name": "وَإِذَا الْجَحِيمُ سُعِّرَتْ"
  },
  {
    "id": 13,
    "name": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ"
  },
  {
    "id": 14,
    "name": "عَلِمَتْ نَفْسٌ مَا أَحْضَرَتْ"
  },
  {
    "id": 15,
    "name": "فَلَا أُقْسِمُ بِالْخُنَّسِ"
  },
  {
    "id": 16,
    "name": "الْجَوَارِ الْكُنَّسِ"
  },
  {
    "id": 17,
    "name": "وَاللَّيْلِ إِذَا عَسْعَسَ"
  },
  {
    "id": 18,
    "name": "وَالصُّبْحِ إِذَا تَنَفَّسَ"
  },
  {
    "id": 19,
    "name": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ"
  },
  {
    "id": 20,
    "name": "ذِي قُوَّةٍ عِنْدَ ذِي الْعَرْشِ مَكِينٍ"
  },
  {
    "id": 21,
    "name": "مُطَاعٍ ثَمَّ أَمِينٍ"
  },
  {
    "id": 22,
    "name": "وَمَا صَاحِبُكُمْ بِمَجْنُونٍ"
  },
  {
    "id": 23,
    "name": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ"
  },
  {
    "id": 24,
    "name": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ"
  },
  {
    "id": 25,
    "name": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَجِيمٍ"
  },
  {
    "id": 26,
    "name": "فَأَيْنَ تَذْهَبُونَ"
  },
  {
    "id": 27,
    "name": "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ"
  },
  {
    "id": 28,
    "name": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَسْتَقِيمَ"
  },
  {
    "id": 29,
    "name": "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ"
  }
]