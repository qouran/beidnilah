[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَاللَّيْلِ إِذَا يَغْشَىٰ"
  },
  {
    "id": 2,
    "name": "وَالنَّهَارِ إِذَا تَجَلَّىٰ"
  },
  {
    "id": 3,
    "name": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنْثَىٰ"
  },
  {
    "id": 4,
    "name": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ"
  },
  {
    "id": 5,
    "name": "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ"
  },
  {
    "id": 6,
    "name": "وَصَدَّقَ بِالْحُسْنَىٰ"
  },
  {
    "id": 7,
    "name": "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ"
  },
  {
    "id": 8,
    "name": "وَأَمَّا مَنْ بَخِلَ وَاسْتَغْنَىٰ"
  },
  {
    "id": 9,
    "name": "وَكَذَّبَ بِالْحُسْنَىٰ"
  },
  {
    "id": 10,
    "name": "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ"
  },
  {
    "id": 11,
    "name": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ"
  },
  {
    "id": 12,
    "name": "إِنَّ عَلَيْنَا لَلْهُدَىٰ"
  },
  {
    "id": 13,
    "name": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ"
  },
  {
    "id": 14,
    "name": "فَأَنْذَرْتُكُمْ نَارًا تَلَظَّىٰ"
  },
  {
    "id": 15,
    "name": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى"
  },
  {
    "id": 16,
    "name": "الَّذِي كَذَّبَ وَتَوَلَّىٰ"
  },
  {
    "id": 17,
    "name": "وَسَيُجَنَّبُهَا الْأَتْقَى"
  },
  {
    "id": 18,
    "name": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ"
  },
  {
    "id": 19,
    "name": "وَمَا لِأَحَدٍ عِنْدَهُ مِنْ نِعْمَةٍ تُجْزَىٰ"
  },
  {
    "id": 20,
    "name": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ"
  },
  {
    "id": 21,
    "name": "وَلَسَوْفَ يَرْضَىٰ"
  }
]