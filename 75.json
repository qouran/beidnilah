[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ"
  },
  {
    "id": 2,
    "name": "وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ"
  },
  {
    "id": 3,
    "name": "أَيَحْسَبُ الْإِنْسَانُ أَلَّنْ نَجْمَعَ عِظَامَهُ"
  },
  {
    "id": 4,
    "name": "بَلَىٰ قَادِرِينَ عَلَىٰ أَنْ نُسَوِّيَ بَنَانَهُ"
  },
  {
    "id": 5,
    "name": "بَلْ يُرِيدُ الْإِنْسَانُ لِيَفْجُرَ أَمَامَهُ"
  },
  {
    "id": 6,
    "name": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ"
  },
  {
    "id": 7,
    "name": "فَإِذَا بَرِقَ الْبَصَرُ"
  },
  {
    "id": 8,
    "name": "وَخَسَفَ الْقَمَرُ"
  },
  {
    "id": 9,
    "name": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ"
  },
  {
    "id": 10,
    "name": "يَقُولُ الْإِنْسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ"
  },
  {
    "id": 11,
    "name": "كَلَّا لَا وَزَرَ"
  },
  {
    "id": 12,
    "name": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ"
  },
  {
    "id": 13,
    "name": "يُنَبَّأُ الْإِنْسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ"
  },
  {
    "id": 14,
    "name": "بَلِ الْإِنْسَانُ عَلَىٰ نَفْسِهِ بَصِيرَةٌ"
  },
  {
    "id": 15,
    "name": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُ"
  },
  {
    "id": 16,
    "name": "لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ"
  },
  {
    "id": 17,
    "name": "إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ"
  },
  {
    "id": 18,
    "name": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ"
  },
  {
    "id": 19,
    "name": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ"
  },
  {
    "id": 20,
    "name": "كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ"
  },
  {
    "id": 21,
    "name": "وَتَذَرُونَ الْآخِرَةَ"
  },
  {
    "id": 22,
    "name": "وُجُوهٌ يَوْمَئِذٍ نَاضِرَةٌ"
  },
  {
    "id": 23,
    "name": "إِلَىٰ رَبِّهَا نَاظِرَةٌ"
  },
  {
    "id": 24,
    "name": "وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ"
  },
  {
    "id": 25,
    "name": "تَظُنُّ أَنْ يُفْعَلَ بِهَا فَاقِرَةٌ"
  },
  {
    "id": 26,
    "name": "كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ"
  },
  {
    "id": 27,
    "name": "وَقِيلَ مَنْ ۜ رَاقٍ"
  },
  {
    "id": 28,
    "name": "وَظَنَّ أَنَّهُ الْفِرَاقُ"
  },
  {
    "id": 29,
    "name": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ"
  },
  {
    "id": 30,
    "name": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ"
  },
  {
    "id": 31,
    "name": "فَلَا صَدَّقَ وَلَا صَلَّىٰ"
  },
  {
    "id": 32,
    "name": "وَلَٰكِنْ كَذَّبَ وَتَوَلَّىٰ"
  },
  {
    "id": 33,
    "name": "ثُمَّ ذَهَبَ إِلَىٰ أَهْلِهِ يَتَمَطَّىٰ"
  },
  {
    "id": 34,
    "name": "أَوْلَىٰ لَكَ فَأَوْلَىٰ"
  },
  {
    "id": 35,
    "name": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰ"
  },
  {
    "id": 36,
    "name": "أَيَحْسَبُ الْإِنْسَانُ أَنْ يُتْرَكَ سُدًى"
  },
  {
    "id": 37,
    "name": "أَلَمْ يَكُ نُطْفَةً مِنْ مَنِيٍّ يُمْنَىٰ"
  },
  {
    "id": 38,
    "name": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ"
  },
  {
    "id": 39,
    "name": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنْثَىٰ"
  },
  {
    "id": 40,
    "name": "أَلَيْسَ ذَٰلِكَ بِقَادِرٍ عَلَىٰ أَنْ يُحْيِيَ الْمَوْتَىٰ"
  }
]