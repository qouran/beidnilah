[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "إِذَا وَقَعَتِ الْوَاقِعَةُ"
  },
  {
    "id": 2,
    "name": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ"
  },
  {
    "id": 3,
    "name": "خَافِضَةٌ رَافِعَةٌ"
  },
  {
    "id": 4,
    "name": "إِذَا رُجَّتِ الْأَرْضُ رَجًّا"
  },
  {
    "id": 5,
    "name": "وَبُسَّتِ الْجِبَالُ بَسًّا"
  },
  {
    "id": 6,
    "name": "فَكَانَتْ هَبَاءً مُنْبَثًّا"
  },
  {
    "id": 7,
    "name": "وَكُنْتُمْ أَزْوَاجًا ثَلَاثَةً"
  },
  {
    "id": 8,
    "name": "فَأَصْحَابُ الْمَيْمَنَةِ مَا أَصْحَابُ الْمَيْمَنَةِ"
  },
  {
    "id": 9,
    "name": "وَأَصْحَابُ الْمَشْأَمَةِ مَا أَصْحَابُ الْمَشْأَمَةِ"
  },
  {
    "id": 10,
    "name": "وَالسَّابِقُونَ السَّابِقُونَ"
  },
  {
    "id": 11,
    "name": "أُولَٰئِكَ الْمُقَرَّبُونَ"
  },
  {
    "id": 12,
    "name": "فِي جَنَّاتِ النَّعِيمِ"
  },
  {
    "id": 13,
    "name": "ثُلَّةٌ مِنَ الْأَوَّلِينَ"
  },
  {
    "id": 14,
    "name": "وَقَلِيلٌ مِنَ الْآخِرِينَ"
  },
  {
    "id": 15,
    "name": "عَلَىٰ سُرُرٍ مَوْضُونَةٍ"
  },
  {
    "id": 16,
    "name": "مُتَّكِئِينَ عَلَيْهَا مُتَقَابِلِينَ"
  },
  {
    "id": 17,
    "name": "يَطُوفُ عَلَيْهِمْ وِلْدَانٌ مُخَلَّدُونَ"
  },
  {
    "id": 18,
    "name": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِنْ مَعِينٍ"
  },
  {
    "id": 19,
    "name": "لَا يُصَدَّعُونَ عَنْهَا وَلَا يُنْزِفُونَ"
  },
  {
    "id": 20,
    "name": "وَفَاكِهَةٍ مِمَّا يَتَخَيَّرُونَ"
  },
  {
    "id": 21,
    "name": "وَلَحْمِ طَيْرٍ مِمَّا يَشْتَهُونَ"
  },
  {
    "id": 22,
    "name": "وَحُورٌ عِينٌ"
  },
  {
    "id": 23,
    "name": "كَأَمْثَالِ اللُّؤْلُؤِ الْمَكْنُونِ"
  },
  {
    "id": 24,
    "name": "جَزَاءً بِمَا كَانُوا يَعْمَلُونَ"
  },
  {
    "id": 25,
    "name": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا"
  },
  {
    "id": 26,
    "name": "إِلَّا قِيلًا سَلَامًا سَلَامًا"
  },
  {
    "id": 27,
    "name": "وَأَصْحَابُ الْيَمِينِ مَا أَصْحَابُ الْيَمِينِ"
  },
  {
    "id": 28,
    "name": "فِي سِدْرٍ مَخْضُودٍ"
  },
  {
    "id": 29,
    "name": "وَطَلْحٍ مَنْضُودٍ"
  },
  {
    "id": 30,
    "name": "وَظِلٍّ مَمْدُودٍ"
  },
  {
    "id": 31,
    "name": "وَمَاءٍ مَسْكُوبٍ"
  },
  {
    "id": 32,
    "name": "وَفَاكِهَةٍ كَثِيرَةٍ"
  },
  {
    "id": 33,
    "name": "لَا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ"
  },
  {
    "id": 34,
    "name": "وَفُرُشٍ مَرْفُوعَةٍ"
  },
  {
    "id": 35,
    "name": "إِنَّا أَنْشَأْنَاهُنَّ إِنْشَاءً"
  },
  {
    "id": 36,
    "name": "فَجَعَلْنَاهُنَّ أَبْكَارًا"
  },
  {
    "id": 37,
    "name": "عُرُبًا أَتْرَابًا"
  },
  {
    "id": 38,
    "name": "لِأَصْحَابِ الْيَمِينِ"
  },
  {
    "id": 39,
    "name": "ثُلَّةٌ مِنَ الْأَوَّلِينَ"
  },
  {
    "id": 40,
    "name": "وَثُلَّةٌ مِنَ الْآخِرِينَ"
  },
  {
    "id": 41,
    "name": "وَأَصْحَابُ الشِّمَالِ مَا أَصْحَابُ الشِّمَالِ"
  },
  {
    "id": 42,
    "name": "فِي سَمُومٍ وَحَمِيمٍ"
  },
  {
    "id": 43,
    "name": "وَظِلٍّ مِنْ يَحْمُومٍ"
  },
  {
    "id": 44,
    "name": "لَا بَارِدٍ وَلَا كَرِيمٍ"
  },
  {
    "id": 45,
    "name": "إِنَّهُمْ كَانُوا قَبْلَ ذَٰلِكَ مُتْرَفِينَ"
  },
  {
    "id": 46,
    "name": "وَكَانُوا يُصِرُّونَ عَلَى الْحِنْثِ الْعَظِيمِ"
  },
  {
    "id": 47,
    "name": "وَكَانُوا يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ"
  },
  {
    "id": 48,
    "name": "أَوَآبَاؤُنَا الْأَوَّلُونَ"
  },
  {
    "id": 49,
    "name": "قُلْ إِنَّ الْأَوَّلِينَ وَالْآخِرِينَ"
  },
  {
    "id": 50,
    "name": "لَمَجْمُوعُونَ إِلَىٰ مِيقَاتِ يَوْمٍ مَعْلُومٍ"
  },
  {
    "id": 51,
    "name": "ثُمَّ إِنَّكُمْ أَيُّهَا الضَّالُّونَ الْمُكَذِّبُونَ"
  },
  {
    "id": 52,
    "name": "لَآكِلُونَ مِنْ شَجَرٍ مِنْ زَقُّومٍ"
  },
  {
    "id": 53,
    "name": "فَمَالِئُونَ مِنْهَا الْبُطُونَ"
  },
  {
    "id": 54,
    "name": "فَشَارِبُونَ عَلَيْهِ مِنَ الْحَمِيمِ"
  },
  {
    "id": 55,
    "name": "فَشَارِبُونَ شُرْبَ الْهِيمِ"
  },
  {
    "id": 56,
    "name": "هَٰذَا نُزُلُهُمْ يَوْمَ الدِّينِ"
  },
  {
    "id": 57,
    "name": "نَحْنُ خَلَقْنَاكُمْ فَلَوْلَا تُصَدِّقُونَ"
  },
  {
    "id": 58,
    "name": "أَفَرَأَيْتُمْ مَا تُمْنُونَ"
  },
  {
    "id": 59,
    "name": "أَأَنْتُمْ تَخْلُقُونَهُ أَمْ نَحْنُ الْخَالِقُونَ"
  },
  {
    "id": 60,
    "name": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"
  },
  {
    "id": 61,
    "name": "عَلَىٰ أَنْ نُبَدِّلَ أَمْثَالَكُمْ وَنُنْشِئَكُمْ فِي مَا لَا تَعْلَمُونَ"
  },
  {
    "id": 62,
    "name": "وَلَقَدْ عَلِمْتُمُ النَّشْأَةَ الْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ"
  },
  {
    "id": 63,
    "name": "أَفَرَأَيْتُمْ مَا تَحْرُثُونَ"
  },
  {
    "id": 64,
    "name": "أَأَنْتُمْ تَزْرَعُونَهُ أَمْ نَحْنُ الزَّارِعُونَ"
  },
  {
    "id": 65,
    "name": "لَوْ نَشَاءُ لَجَعَلْنَاهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُونَ"
  },
  {
    "id": 66,
    "name": "إِنَّا لَمُغْرَمُونَ"
  },
  {
    "id": 67,
    "name": "بَلْ نَحْنُ مَحْرُومُونَ"
  },
  {
    "id": 68,
    "name": "أَفَرَأَيْتُمُ الْمَاءَ الَّذِي تَشْرَبُونَ"
  },
  {
    "id": 69,
    "name": "أَأَنْتُمْ أَنْزَلْتُمُوهُ مِنَ الْمُزْنِ أَمْ نَحْنُ الْمُنْزِلُونَ"
  },
  {
    "id": 70,
    "name": "لَوْ نَشَاءُ جَعَلْنَاهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ"
  },
  {
    "id": 71,
    "name": "أَفَرَأَيْتُمُ النَّارَ الَّتِي تُورُونَ"
  },
  {
    "id": 72,
    "name": "أَأَنْتُمْ أَنْشَأْتُمْ شَجَرَتَهَا أَمْ نَحْنُ الْمُنْشِئُونَ"
  },
  {
    "id": 73,
    "name": "نَحْنُ جَعَلْنَاهَا تَذْكِرَةً وَمَتَاعًا لِلْمُقْوِينَ"
  },
  {
    "id": 74,
    "name": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
  },
  {
    "id": 75,
    "name": "۞ فَلَا أُقْسِمُ بِمَوَاقِعِ النُّجُومِ"
  },
  {
    "id": 76,
    "name": "وَإِنَّهُ لَقَسَمٌ لَوْ تَعْلَمُونَ عَظِيمٌ"
  },
  {
    "id": 77,
    "name": "إِنَّهُ لَقُرْآنٌ كَرِيمٌ"
  },
  {
    "id": 78,
    "name": "فِي كِتَابٍ مَكْنُونٍ"
  },
  {
    "id": 79,
    "name": "لَا يَمَسُّهُ إِلَّا الْمُطَهَّرُونَ"
  },
  {
    "id": 80,
    "name": "تَنْزِيلٌ مِنْ رَبِّ الْعَالَمِينَ"
  },
  {
    "id": 81,
    "name": "أَفَبِهَٰذَا الْحَدِيثِ أَنْتُمْ مُدْهِنُونَ"
  },
  {
    "id": 82,
    "name": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ"
  },
  {
    "id": 83,
    "name": "فَلَوْلَا إِذَا بَلَغَتِ الْحُلْقُومَ"
  },
  {
    "id": 84,
    "name": "وَأَنْتُمْ حِينَئِذٍ تَنْظُرُونَ"
  },
  {
    "id": 85,
    "name": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْكُمْ وَلَٰكِنْ لَا تُبْصِرُونَ"
  },
  {
    "id": 86,
    "name": "فَلَوْلَا إِنْ كُنْتُمْ غَيْرَ مَدِينِينَ"
  },
  {
    "id": 87,
    "name": "تَرْجِعُونَهَا إِنْ كُنْتُمْ صَادِقِينَ"
  },
  {
    "id": 88,
    "name": "فَأَمَّا إِنْ كَانَ مِنَ الْمُقَرَّبِينَ"
  },
  {
    "id": 89,
    "name": "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ"
  },
  {
    "id": 90,
    "name": "وَأَمَّا إِنْ كَانَ مِنْ أَصْحَابِ الْيَمِينِ"
  },
  {
    "id": 91,
    "name": "فَسَلَامٌ لَكَ مِنْ أَصْحَابِ الْيَمِينِ"
  },
  {
    "id": 92,
    "name": "وَأَمَّا إِنْ كَانَ مِنَ الْمُكَذِّبِينَ الضَّالِّينَ"
  },
  {
    "id": 93,
    "name": "فَنُزُلٌ مِنْ حَمِيمٍ"
  },
  {
    "id": 94,
    "name": "وَتَصْلِيَةُ جَحِيمٍ"
  },
  {
    "id": 95,
    "name": "إِنَّ هَٰذَا لَهُوَ حَقُّ الْيَقِينِ"
  },
  {
    "id": 96,
    "name": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيمِ"
  }
]