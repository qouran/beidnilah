[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "سَأَلَ سَائِلٌ بِعَذَابٍ وَاقِعٍ"
  },
  {
    "id": 2,
    "name": "لِلْكَافِرِينَ لَيْسَ لَهُ دَافِعٌ"
  },
  {
    "id": 3,
    "name": "مِنَ اللَّهِ ذِي الْمَعَارِجِ"
  },
  {
    "id": 4,
    "name": "تَعْرُجُ الْمَلَائِكَةُ وَالرُّوحُ إِلَيْهِ فِي يَوْمٍ كَانَ مِقْدَارُهُ خَمْسِينَ أَلْفَ سَنَةٍ"
  },
  {
    "id": 5,
    "name": "فَاصْبِرْ صَبْرًا جَمِيلًا"
  },
  {
    "id": 6,
    "name": "إِنَّهُمْ يَرَوْنَهُ بَعِيدًا"
  },
  {
    "id": 7,
    "name": "وَنَرَاهُ قَرِيبًا"
  },
  {
    "id": 8,
    "name": "يَوْمَ تَكُونُ السَّمَاءُ كَالْمُهْلِ"
  },
  {
    "id": 9,
    "name": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ"
  },
  {
    "id": 10,
    "name": "وَلَا يَسْأَلُ حَمِيمٌ حَمِيمًا"
  },
  {
    "id": 11,
    "name": "يُبَصَّرُونَهُمْ ۚ يَوَدُّ الْمُجْرِمُ لَوْ يَفْتَدِي مِنْ عَذَابِ يَوْمِئِذٍ بِبَنِيهِ"
  },
  {
    "id": 12,
    "name": "وَصَاحِبَتِهِ وَأَخِيهِ"
  },
  {
    "id": 13,
    "name": "وَفَصِيلَتِهِ الَّتِي تُؤْوِيهِ"
  },
  {
    "id": 14,
    "name": "وَمَنْ فِي الْأَرْضِ جَمِيعًا ثُمَّ يُنْجِيهِ"
  },
  {
    "id": 15,
    "name": "كَلَّا ۖ إِنَّهَا لَظَىٰ"
  },
  {
    "id": 16,
    "name": "نَزَّاعَةً لِلشَّوَىٰ"
  },
  {
    "id": 17,
    "name": "تَدْعُو مَنْ أَدْبَرَ وَتَوَلَّىٰ"
  },
  {
    "id": 18,
    "name": "وَجَمَعَ فَأَوْعَىٰ"
  },
  {
    "id": 19,
    "name": "۞ إِنَّ الْإِنْسَانَ خُلِقَ هَلُوعًا"
  },
  {
    "id": 20,
    "name": "إِذَا مَسَّهُ الشَّرُّ جَزُوعًا"
  },
  {
    "id": 21,
    "name": "وَإِذَا مَسَّهُ الْخَيْرُ مَنُوعًا"
  },
  {
    "id": 22,
    "name": "إِلَّا الْمُصَلِّينَ"
  },
  {
    "id": 23,
    "name": "الَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَائِمُونَ"
  },
  {
    "id": 24,
    "name": "وَالَّذِينَ فِي أَمْوَالِهِمْ حَقٌّ مَعْلُومٌ"
  },
  {
    "id": 25,
    "name": "لِلسَّائِلِ وَالْمَحْرُومِ"
  },
  {
    "id": 26,
    "name": "وَالَّذِينَ يُصَدِّقُونَ بِيَوْمِ الدِّينِ"
  },
  {
    "id": 27,
    "name": "وَالَّذِينَ هُمْ مِنْ عَذَابِ رَبِّهِمْ مُشْفِقُونَ"
  },
  {
    "id": 28,
    "name": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ"
  },
  {
    "id": 29,
    "name": "وَالَّذِينَ هُمْ لِفُرُوجِهِمْ حَافِظُونَ"
  },
  {
    "id": 30,
    "name": "إِلَّا عَلَىٰ أَزْوَاجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَانُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ"
  },
  {
    "id": 31,
    "name": "فَمَنِ ابْتَغَىٰ وَرَاءَ ذَٰلِكَ فَأُولَٰئِكَ هُمُ الْعَادُونَ"
  },
  {
    "id": 32,
    "name": "وَالَّذِينَ هُمْ لِأَمَانَاتِهِمْ وَعَهْدِهِمْ رَاعُونَ"
  },
  {
    "id": 33,
    "name": "وَالَّذِينَ هُمْ بِشَهَادَاتِهِمْ قَائِمُونَ"
  },
  {
    "id": 34,
    "name": "وَالَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ"
  },
  {
    "id": 35,
    "name": "أُولَٰئِكَ فِي جَنَّاتٍ مُكْرَمُونَ"
  },
  {
    "id": 36,
    "name": "فَمَالِ الَّذِينَ كَفَرُوا قِبَلَكَ مُهْطِعِينَ"
  },
  {
    "id": 37,
    "name": "عَنِ الْيَمِينِ وَعَنِ الشِّمَالِ عِزِينَ"
  },
  {
    "id": 38,
    "name": "أَيَطْمَعُ كُلُّ امْرِئٍ مِنْهُمْ أَنْ يُدْخَلَ جَنَّةَ نَعِيمٍ"
  },
  {
    "id": 39,
    "name": "كَلَّا ۖ إِنَّا خَلَقْنَاهُمْ مِمَّا يَعْلَمُونَ"
  },
  {
    "id": 40,
    "name": "فَلَا أُقْسِمُ بِرَبِّ الْمَشَارِقِ وَالْمَغَارِبِ إِنَّا لَقَادِرُونَ"
  },
  {
    "id": 41,
    "name": "عَلَىٰ أَنْ نُبَدِّلَ خَيْرًا مِنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ"
  },
  {
    "id": 42,
    "name": "فَذَرْهُمْ يَخُوضُوا وَيَلْعَبُوا حَتَّىٰ يُلَاقُوا يَوْمَهُمُ الَّذِي يُوعَدُونَ"
  },
  {
    "id": 43,
    "name": "يَوْمَ يَخْرُجُونَ مِنَ الْأَجْدَاثِ سِرَاعًا كَأَنَّهُمْ إِلَىٰ نُصُبٍ يُوفِضُونَ"
  },
  {
    "id": 44,
    "name": "خَاشِعَةً أَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۚ ذَٰلِكَ الْيَوْمُ الَّذِي كَانُوا يُوعَدُونَ"
  }
]