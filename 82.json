[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "إِذَا السَّمَاءُ انْفَطَرَتْ"
  },
  {
    "id": 2,
    "name": "وَإِذَا الْكَوَاكِبُ انْتَثَرَتْ"
  },
  {
    "id": 3,
    "name": "وَإِذَا الْبِحَارُ فُجِّرَتْ"
  },
  {
    "id": 4,
    "name": "وَإِذَا الْقُبُورُ بُعْثِرَتْ"
  },
  {
    "id": 5,
    "name": "عَلِمَتْ نَفْسٌ مَا قَدَّمَتْ وَأَخَّرَتْ"
  },
  {
    "id": 6,
    "name": "يَا أَيُّهَا الْإِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِيمِ"
  },
  {
    "id": 7,
    "name": "الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَكَ"
  },
  {
    "id": 8,
    "name": "فِي أَيِّ صُورَةٍ مَا شَاءَ رَكَّبَكَ"
  },
  {
    "id": 9,
    "name": "كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ"
  },
  {
    "id": 10,
    "name": "وَإِنَّ عَلَيْكُمْ لَحَافِظِينَ"
  },
  {
    "id": 11,
    "name": "كِرَامًا كَاتِبِينَ"
  },
  {
    "id": 12,
    "name": "يَعْلَمُونَ مَا تَفْعَلُونَ"
  },
  {
    "id": 13,
    "name": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ"
  },
  {
    "id": 14,
    "name": "وَإِنَّ الْفُجَّارَ لَفِي جَحِيمٍ"
  },
  {
    "id": 15,
    "name": "يَصْلَوْنَهَا يَوْمَ الدِّينِ"
  },
  {
    "id": 16,
    "name": "وَمَا هُمْ عَنْهَا بِغَائِبِينَ"
  },
  {
    "id": 17,
    "name": "وَمَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
  },
  {
    "id": 18,
    "name": "ثُمَّ مَا أَدْرَاكَ مَا يَوْمُ الدِّينِ"
  },
  {
    "id": 19,
    "name": "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِنَفْسٍ شَيْئًا ۖ وَالْأَمْرُ يَوْمَئِذٍ لِلَّهِ"
  }
]