[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "إِذَا زُلْزِلَتِ الْأَرْضُ زِلْزَالَهَا"
  },
  {
    "id": 2,
    "name": "وَأَخْرَجَتِ الْأَرْضُ أَثْقَالَهَا"
  },
  {
    "id": 3,
    "name": "وَقَالَ الْإِنْسَانُ مَا لَهَا"
  },
  {
    "id": 4,
    "name": "يَوْمَئِذٍ تُحَدِّثُ أَخْبَارَهَا"
  },
  {
    "id": 5,
    "name": "بِأَنَّ رَبَّكَ أَوْحَىٰ لَهَا"
  },
  {
    "id": 6,
    "name": "يَوْمَئِذٍ يَصْدُرُ النَّاسُ أَشْتَاتًا لِيُرَوْا أَعْمَالَهُمْ"
  },
  {
    "id": 7,
    "name": "فَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُ"
  },
  {
    "id": 8,
    "name": "وَمَنْ يَعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَرَهُ"
  }
]