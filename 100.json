[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالْعَادِيَاتِ ضَبْحًا"
  },
  {
    "id": 2,
    "name": "فَالْمُورِيَاتِ قَدْحًا"
  },
  {
    "id": 3,
    "name": "فَالْمُغِيرَاتِ صُبْحًا"
  },
  {
    "id": 4,
    "name": "فَأَثَرْنَ بِهِ نَقْعًا"
  },
  {
    "id": 5,
    "name": "فَوَسَطْنَ بِهِ جَمْعًا"
  },
  {
    "id": 6,
    "name": "إِنَّ الْإِنْسَانَ لِرَبِّهِ لَكَنُودٌ"
  },
  {
    "id": 7,
    "name": "وَإِنَّهُ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ"
  },
  {
    "id": 8,
    "name": "وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ"
  },
  {
    "id": 9,
    "name": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ"
  },
  {
    "id": 10,
    "name": "وَحُصِّلَ مَا فِي الصُّدُورِ"
  },
  {
    "id": 11,
    "name": "إِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَخَبِيرٌ"
  }
]