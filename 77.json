[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالْمُرْسَلَاتِ عُرْفًا"
  },
  {
    "id": 2,
    "name": "فَالْعَاصِفَاتِ عَصْفًا"
  },
  {
    "id": 3,
    "name": "وَالنَّاشِرَاتِ نَشْرًا"
  },
  {
    "id": 4,
    "name": "فَالْفَارِقَاتِ فَرْقًا"
  },
  {
    "id": 5,
    "name": "فَالْمُلْقِيَاتِ ذِكْرًا"
  },
  {
    "id": 6,
    "name": "عُذْرًا أَوْ نُذْرًا"
  },
  {
    "id": 7,
    "name": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ"
  },
  {
    "id": 8,
    "name": "فَإِذَا النُّجُومُ طُمِسَتْ"
  },
  {
    "id": 9,
    "name": "وَإِذَا السَّمَاءُ فُرِجَتْ"
  },
  {
    "id": 10,
    "name": "وَإِذَا الْجِبَالُ نُسِفَتْ"
  },
  {
    "id": 11,
    "name": "وَإِذَا الرُّسُلُ أُقِّتَتْ"
  },
  {
    "id": 12,
    "name": "لِأَيِّ يَوْمٍ أُجِّلَتْ"
  },
  {
    "id": 13,
    "name": "لِيَوْمِ الْفَصْلِ"
  },
  {
    "id": 14,
    "name": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ"
  },
  {
    "id": 15,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 16,
    "name": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ"
  },
  {
    "id": 17,
    "name": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ"
  },
  {
    "id": 18,
    "name": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ"
  },
  {
    "id": 19,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 20,
    "name": "أَلَمْ نَخْلُقْكُمْ مِنْ مَاءٍ مَهِينٍ"
  },
  {
    "id": 21,
    "name": "فَجَعَلْنَاهُ فِي قَرَارٍ مَكِينٍ"
  },
  {
    "id": 22,
    "name": "إِلَىٰ قَدَرٍ مَعْلُومٍ"
  },
  {
    "id": 23,
    "name": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ"
  },
  {
    "id": 24,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 25,
    "name": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا"
  },
  {
    "id": 26,
    "name": "أَحْيَاءً وَأَمْوَاتًا"
  },
  {
    "id": 27,
    "name": "وَجَعَلْنَا فِيهَا رَوَاسِيَ شَامِخَاتٍ وَأَسْقَيْنَاكُمْ مَاءً فُرَاتًا"
  },
  {
    "id": 28,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 29,
    "name": "انْطَلِقُوا إِلَىٰ مَا كُنْتُمْ بِهِ تُكَذِّبُونَ"
  },
  {
    "id": 30,
    "name": "انْطَلِقُوا إِلَىٰ ظِلٍّ ذِي ثَلَاثِ شُعَبٍ"
  },
  {
    "id": 31,
    "name": "لَا ظَلِيلٍ وَلَا يُغْنِي مِنَ اللَّهَبِ"
  },
  {
    "id": 32,
    "name": "إِنَّهَا تَرْمِي بِشَرَرٍ كَالْقَصْرِ"
  },
  {
    "id": 33,
    "name": "كَأَنَّهُ جِمَالَتٌ صُفْرٌ"
  },
  {
    "id": 34,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 35,
    "name": "هَٰذَا يَوْمُ لَا يَنْطِقُونَ"
  },
  {
    "id": 36,
    "name": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ"
  },
  {
    "id": 37,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 38,
    "name": "هَٰذَا يَوْمُ الْفَصْلِ ۖ جَمَعْنَاكُمْ وَالْأَوَّلِينَ"
  },
  {
    "id": 39,
    "name": "فَإِنْ كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ"
  },
  {
    "id": 40,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 41,
    "name": "إِنَّ الْمُتَّقِينَ فِي ظِلَالٍ وَعُيُونٍ"
  },
  {
    "id": 42,
    "name": "وَفَوَاكِهَ مِمَّا يَشْتَهُونَ"
  },
  {
    "id": 43,
    "name": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنْتُمْ تَعْمَلُونَ"
  },
  {
    "id": 44,
    "name": "إِنَّا كَذَٰلِكَ نَجْزِي الْمُحْسِنِينَ"
  },
  {
    "id": 45,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 46,
    "name": "كُلُوا وَتَمَتَّعُوا قَلِيلًا إِنَّكُمْ مُجْرِمُونَ"
  },
  {
    "id": 47,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 48,
    "name": "وَإِذَا قِيلَ لَهُمُ ارْكَعُوا لَا يَرْكَعُونَ"
  },
  {
    "id": 49,
    "name": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ"
  },
  {
    "id": 50,
    "name": "فَبِأَيِّ حَدِيثٍ بَعْدَهُ يُؤْمِنُونَ"
  }
]