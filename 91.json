[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالشَّمْسِ وَضُحَاهَا"
  },
  {
    "id": 2,
    "name": "وَالْقَمَرِ إِذَا تَلَاهَا"
  },
  {
    "id": 3,
    "name": "وَالنَّهَارِ إِذَا جَلَّاهَا"
  },
  {
    "id": 4,
    "name": "وَاللَّيْلِ إِذَا يَغْشَاهَا"
  },
  {
    "id": 5,
    "name": "وَالسَّمَاءِ وَمَا بَنَاهَا"
  },
  {
    "id": 6,
    "name": "وَالْأَرْضِ وَمَا طَحَاهَا"
  },
  {
    "id": 7,
    "name": "وَنَفْسٍ وَمَا سَوَّاهَا"
  },
  {
    "id": 8,
    "name": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا"
  },
  {
    "id": 9,
    "name": "قَدْ أَفْلَحَ مَنْ زَكَّاهَا"
  },
  {
    "id": 10,
    "name": "وَقَدْ خَابَ مَنْ دَسَّاهَا"
  },
  {
    "id": 11,
    "name": "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا"
  },
  {
    "id": 12,
    "name": "إِذِ انْبَعَثَ أَشْقَاهَا"
  },
  {
    "id": 13,
    "name": "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا"
  },
  {
    "id": 14,
    "name": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُمْ بِذَنْبِهِمْ فَسَوَّاهَا"
  },
  {
    "id": 15,
    "name": "وَلَا يَخَافُ عُقْبَاهَا"
  }
]