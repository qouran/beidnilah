[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالْفَجْرِ"
  },
  {
    "id": 2,
    "name": "وَلَيَالٍ عَشْرٍ"
  },
  {
    "id": 3,
    "name": "وَالشَّفْعِ وَالْوَتْرِ"
  },
  {
    "id": 4,
    "name": "وَاللَّيْلِ إِذَا يَسْرِ"
  },
  {
    "id": 5,
    "name": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِذِي حِجْرٍ"
  },
  {
    "id": 6,
    "name": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ"
  },
  {
    "id": 7,
    "name": "إِرَمَ ذَاتِ الْعِمَادِ"
  },
  {
    "id": 8,
    "name": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ"
  },
  {
    "id": 9,
    "name": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ"
  },
  {
    "id": 10,
    "name": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ"
  },
  {
    "id": 11,
    "name": "الَّذِينَ طَغَوْا فِي الْبِلَادِ"
  },
  {
    "id": 12,
    "name": "فَأَكْثَرُوا فِيهَا الْفَسَادَ"
  },
  {
    "id": 13,
    "name": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ"
  },
  {
    "id": 14,
    "name": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ"
  },
  {
    "id": 15,
    "name": "فَأَمَّا الْإِنْسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ"
  },
  {
    "id": 16,
    "name": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ"
  },
  {
    "id": 17,
    "name": "كَلَّا ۖ بَلْ لَا تُكْرِمُونَ الْيَتِيمَ"
  },
  {
    "id": 18,
    "name": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ"
  },
  {
    "id": 19,
    "name": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَمًّا"
  },
  {
    "id": 20,
    "name": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا"
  },
  {
    "id": 21,
    "name": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا"
  },
  {
    "id": 22,
    "name": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا"
  },
  {
    "id": 23,
    "name": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنْسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ"
  },
  {
    "id": 24,
    "name": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي"
  },
  {
    "id": 25,
    "name": "فَيَوْمَئِذٍ لَا يُعَذِّبُ عَذَابَهُ أَحَدٌ"
  },
  {
    "id": 26,
    "name": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ"
  },
  {
    "id": 27,
    "name": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ"
  },
  {
    "id": 28,
    "name": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَرْضِيَّةً"
  },
  {
    "id": 29,
    "name": "فَادْخُلِي فِي عِبَادِي"
  },
  {
    "id": 30,
    "name": "وَادْخُلِي جَنَّتِي"
  }
]