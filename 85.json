[
  {
    "id": 0,
    "name": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ"
  },
  {
    "id": 1,
    "name": "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ"
  },
  {
    "id": 2,
    "name": "وَالْيَوْمِ الْمَوْعُودِ"
  },
  {
    "id": 3,
    "name": "وَشَاهِدٍ وَمَشْهُودٍ"
  },
  {
    "id": 4,
    "name": "قُتِلَ أَصْحَابُ الْأُخْدُودِ"
  },
  {
    "id": 5,
    "name": "النَّارِ ذَاتِ الْوَقُودِ"
  },
  {
    "id": 6,
    "name": "إِذْ هُمْ عَلَيْهَا قُعُودٌ"
  },
  {
    "id": 7,
    "name": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ"
  },
  {
    "id": 8,
    "name": "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَنْ يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ"
  },
  {
    "id": 9,
    "name": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ"
  },
  {
    "id": 10,
    "name": "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ"
  },
  {
    "id": 11,
    "name": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ"
  },
  {
    "id": 12,
    "name": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ"
  },
  {
    "id": 13,
    "name": "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ"
  },
  {
    "id": 14,
    "name": "وَهُوَ الْغَفُورُ الْوَدُودُ"
  },
  {
    "id": 15,
    "name": "ذُو الْعَرْشِ الْمَجِيدُ"
  },
  {
    "id": 16,
    "name": "فَعَّالٌ لِمَا يُرِيدُ"
  },
  {
    "id": 17,
    "name": "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ"
  },
  {
    "id": 18,
    "name": "فِرْعَوْنَ وَثَمُودَ"
  },
  {
    "id": 19,
    "name": "بَلِ الَّذِينَ كَفَرُوا فِي تَكْذِيبٍ"
  },
  {
    "id": 20,
    "name": "وَاللَّهُ مِنْ وَرَائِهِمْ مُحِيطٌ"
  },
  {
    "id": 21,
    "name": "بَلْ هُوَ قُرْآنٌ مَجِيدٌ"
  },
  {
    "id": 22,
    "name": "فِي لَوْحٍ مَحْفُوظٍ"
  }
]